[application]

config/name="Tetris"
run/main_scene="res://MainMenu.tscn"
boot_splash/image="res://tetrissplash.png"
boot_splash/bg_color=Color( 0.309804, 0.219608, 0.2, 1 )
config/icon="res://tetrislogo.png"

[autoload]

UserData="*res://addons/github-integration/scripts/user_data.gd"
